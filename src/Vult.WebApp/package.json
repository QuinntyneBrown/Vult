{
  "name": "vult-web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve vult",
    "start:admin": "ng serve vult-admin",
    "build": "ng build vult",
    "build:admin": "ng build vult-admin",
    "build:all": "ng build vult && ng build vult-admin",
    "watch": "ng build --watch --configuration development",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "e2e": "playwright test",
    "e2e:vult": "playwright test --project=vult",
    "e2e:admin": "playwright test --project=vult-admin"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.4",
  "dependencies": {
    "@angular/animations": "^21.0.0",
    "@angular/cdk": "^21.0.3",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "^21.0.3",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@microsoft/signalr": "^10.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.0",
    "@playwright/test": "^1.57.0",
    "@types/jest": "^30.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "jest-preset-angular": "^16.0.0",
    "typescript": "~5.9.2"
  }
}
